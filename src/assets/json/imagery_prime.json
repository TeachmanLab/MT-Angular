[
  {
    "session": "imageryPrime",
    "title": "",
    "subTitle": "",
    "conditioning": "Training",
    "study": "Calm Thinking",
    "steps": [
      {
        "title": "Use Your Imagination",
        "pages": [
          {
            "elements": [
              {
                "type": "LargeHeader",
                "stimulusName": "Imagination Header",
                "content": "Use Your Imagination"
              },
              {
                "type": "Paragraph",
                "stimulusName": "readiness_p1",
                "content": "The Calm Thinking program requires you to imagine yourself in different situations. We believe that vividly imagining yourself in these situations is important for the program to be effective, so we want you to do a practice imagery exercise before you start the training."
              },
              {
                "type": "Paragraph",
                "stimulusName": "readiness_p2",
                "content": "Please think of an UPCOMING situation that you expect will make you feel anxious, worried, or scared."
              },
              {
                "type": "BulletList",
                "stimulusName": "readiness_p3",
                "title": "",
                "bullets": ["Pick a situation that you worry might go badly, or that you expect will evoke symptoms related to anxiety (e.g., racing heart, sweating)",
                  "Any situation that will make you feel quite anxious or scared will work, as long as it’s a situation you’re likely to actually encounter"
                ]
              }
            ]
          },
              {"elements": [
                {
                  "type": "LargeHeader",
                  "stimulusName": "Imagination Header",
                  "content": "Use Your Imagination"
                },
                {
                "type": "Header",
                "stimulusName": "readiness_heading",
                "content": "Please describe the situation in a few words:"
              },
              {
                "type": "FillInBlank",
                "stimulusName": "readiness_situation",
                "question": "Please describe the situation in a few words.",
                "maxCharacters": 100
              }
            ]
          },
          {
            "elements": [
              {
                "type": "LargeHeader",
                "stimulusName": "Imagination Header",
                "content": "Use Your Imagination"
              },
              {
                "type": "BulletList",
                "stimulusName": "readiness_p3",
                "title": "Please imagine the situation as vividly as possible (i.e., so that it feels very real).",
                "bullets": [
                  "Try to imagine you are actually in the situation.",
                  "Play out the specific parts of the situation in your head.",
                  "Try to imagine using all your senses while you think about this situation. Think about the things you would see, hear, and smell if you were really in that situation.",
                  "Think about the kinds of things you would think or feel if you were actually there."
                ]
              },
              {
                "type": "Paragraph",
                "stimulusName": "readiness_p5",
                "content": "Please click the \"Begin\" button below and play this situation in your head for the next 20 seconds. We will tell you when the 20 seconds are up. Make sure the sound on your computer is not muted."
              },
              {
                "type": "Countdown",
                "stimulusName": "readiness_timer",
                "delayInSeconds": 20,
                "content": "We will tell you when 20 seconds are up."
              }
            ]
          },
          {
            "elements": [{
              "type": "LargeHeader",
              "stimulusName": "Imagination Header",
              "content": "Use Your Imagination"
            },
              {
                "type": "Header",
                "stimulusName": "readiness_heading",
                "content": "How anxious did you feel (at the highest level of anxiety) as you completed the imagery task?"
              },
              {
                "type": "Slider",
                "stimulusName": "readiness_anxiety_question",
                "min": 0,
                "max": 100
              }
            ]
          },
          {
            "elements": [
              {
                "type": "LargeHeader",
                "stimulusName": "Imagination Header",
                "content": "Use Your Imagination"
              },
              {
                "type": "Question",
                "stimulusName": "readiness_vivid_question",
                "question": "How vividly did you imagine the situation?",
                "options": [
                  "Not at all vivid",
                  "Somewhat vivid",
                  "Moderately vivid",
                  "Very vivid",
                  "Totally vivid"
                ],
                "preferNotToAnswer":true
              }
              ]
              },
              {
                "elements": [
                  {
                    "type": "LargeHeader",
                    "stimulusName": "Imagination Header",
                    "content": "Use Your Imagination"
                  },
              {
                "type": "Question",
                "stimulusName": "readiness_badly_question",
                "question": "How likely is it that this situation or task will turn out well versus turn out badly?",
                "options": [
                  "Very likely to turn out well",
                  "Somewhat likely to turn out well",
                  "Neutral             ",
                  "Somewhat likely to turn out badly",
                  "Very likely to turn out badly"
                ],
                "preferNotToAnswer":true
              }
                ]
              },
              {"elements": [
                {
                  "type": "LargeHeader",
                  "stimulusName": "Imagination Header",
                  "content": "Use Your Imagination"
                },
                {
                  "type": "Question",
                  "stimulusName": "readiness_manageable_question",
                  "question": "If this situation did turn out badly, how well or badly would you be able to handle it?",
                  "options": [
                    "Very well",
                    "Somewhat well",
                    "Neutral",
                    "Somewhat badly",
                    "Very badly"
                  ],
                  "preferNotToAnswer":true
                }
                  ]
              }
            ]
          }

        ]
      }
    ]

